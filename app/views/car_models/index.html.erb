<h1>Modelos de veículos</h1>
<table>
  <thead>
    <tr>
      <th>Fabricante</th>
      <th>Nome</th>
      <th>Ano</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @car_models.each do |cm| %>
      <tr id="car-model-<%= cm.id %>">
        <td><%= cm.manufacturer.name %></td>
        <td><%= cm.name %></td>
        <td><%= cm.year %></td>
        <td><%= link_to 'Ver detalhes', cm, id: "details-#{cm.id}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<p><%= link_to 'Registrar modelo de veículo', new_car_model_path %></p>
<p><%= link_to 'Voltar', root_path %></p>



